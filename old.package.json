    "scripts": {
        "build": "node scripts/build.js",
        "start": "node scripts/start.js",
        "lint": "./node_modules/eslint/bin/eslint.js src/ --config .eslintrc",
        "test": "npm run test:client && npm run test:server",
        "test:client": "TEST_ENV=client karma start",
        "test:client:watch": "TEST_ENV=client TEST_WATCH=true karma start",
        "test:server": "TEST_ENV=server mocha --opts .mocharc 'tests/server/**/*.test.{jsx,js}'",
        "test:server:cli": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/junit/test-results.xml TEST_ENV=server mocha  --reporter mocha-junit-reporter --opts .mocharc 'tests/server/**/*.test.{jsx,js}'",
        "test:server:watch": "TEST_ENV=server mocha --watch --opts .mocharc 'tests/server/**/*.test.{jsx,js}'",
        "test-with-jest": "node scripts/test.js --env=jsdom"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js}"
        ],
        "globals": {
            "COMMIT_HASH": "HASH",
            "window": {},
            "Pico": {},
            "stripe": "stripe"
        },
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/node_modules/stripe"
        ],
        "testMatch": [
            "<rootDir>/tests/**/__tests__/**/*.{js}",
            "<rootDir>/tests/snapshots/**/?(*.)(spec|test).{js}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js)$"
        ],
        "moduleFileExtensions": [
            "js",
            "json"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PicoNetworks/widget.git"
    }
}